    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-es2015": "^6.24.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.6.1",
    "eslint-config-kintone": "^1.2.0",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
