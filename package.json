{
  "name": "kintone-vue-pdfview-sample1",
  "version": "0.0.1",
  "description": "Vue.js Sample on kintone",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --https --content-base ./ --inline",
